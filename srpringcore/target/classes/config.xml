<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xmlns:context="http://www.springframework.org/schema/context" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd" >

	<!-- this is our first beans-->

	<bean class="com.springcore.Student" name="student1" >
		<property name="studentId" value="345" />
		<property name="studentName" value="ABC" />
		<property name="studentAddress" value="INDIA" />
	</bean>
	
	<!-- this is our second beans-->
	
	<bean class="com.springcore.Student" name="student2">
		<property name="studentId" value="234" />
		<property name="studentName" value="XYZ" />
		<property name="studentAddress" value="America" />
	</bean>

</beans>